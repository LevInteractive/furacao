{
  "frequency": 3600000,
  "provider": {
    "name": "rackspace",
    "container": "<container-name>",
    "apikey": "<api-key>",
    "username": "<username>"
  },
  "backups": [
    {
      "name": "NameOfFile",
      "dir": "path/to/dir",
      "glob": ["**/*", "!**/node_modules/**"]
    }
  ]
}
